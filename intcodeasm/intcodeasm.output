        on right: 23 24
    argument_expression (19)
        on left: 6 7
        on right: 8 9 11 12
    bp_modifier (20)
        on left: 8 9
        on right: 10
    relative_argument (21)
        on left: 10
        on right: 14
    absolute_argument (22)
        on left: 11
        on right: 13
    immediate_argument (23)
        on left: 12
        on right: 15
    argument (24)
        on left: 13 14 15
        on right: 16 17
    labeled_argument (25)
        on left: 16 17
        on right: 18 19
    argument_list (26)
        on left: 18 19
        on right: 19 20 24
    instruction (27)
